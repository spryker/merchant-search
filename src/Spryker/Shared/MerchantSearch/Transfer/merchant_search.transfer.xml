<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantCollection">
        <property name="merchants" type="Merchant[]" singular="merchants"/>
    </transfer>

    <transfer name="MerchantSearchCollection">
        <property name="nbResults" type="int"/>
        <property name="merchantSearches" type="MerchantSearch[]" singular="merchantSearch"/>
    </transfer>

    <transfer name="MerchantCriteria">
        <property name="isActive" type="bool"/>
        <property name="status" type="string"/>
        <property name="store" type="Store"/>
    </transfer>

    <transfer name="MerchantSearch">
        <property name="idMerchant" type="int"/>
        <property name="merchantKey" type="string"/>
        <property name="name" type="string"/>
        <property name="registrationNumber" type="string"/>
        <property name="email" type="string"/>
        <property name="merchantReference" type="string"/>
        <property name="data" type="array"/>
        <property name="structuredData" type="string"/>
    </transfer>

    <transfer name="Merchant">
    </transfer>

    <transfer name="Store">
        <property name="name" type="string"/>
    </transfer>

</transfers>
